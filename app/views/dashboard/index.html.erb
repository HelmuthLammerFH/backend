<h1>Dashboard</h1>
<div class="row">
  <div class="col-sm-12">
    <h3>Tours Pro Guide</h3>
  </div>
  <div class="col-sm-12">
    <table class="table table-bordered">
      <thead>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Member Since</th>
        <th>Tour Names</th>
      </tr>
      </thead>
      <tbody>
      <% @tours.each do |tour| %>
          <tr>
            <th scope="row"><%= tour.id %></th>
            <td><%= tour.Tourguide.user.firstname %></td>
            <td><%= tour.Tourguide.user.lastname %></td>
            <td><%= tour.Tourguide.tourGuideSince %></td>
            <td><%= tour.Tourguide.user.firstname %></td>
            <td><%= tour.name %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
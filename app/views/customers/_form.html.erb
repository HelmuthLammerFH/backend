<%= form_for(customer) do |f| %>
  <% if customer.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(customer.errors.count, "error") %> prohibited this customer from being saved:</h2>

      <ul>
      <% customer.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :note %>
    <%= f.text_field :note %>
  </div>

  <div class="field">
    <%= f.label :createdFrom %>
    <%= f.text_field :createdFrom %>
  </div>

  <div class="field">
    <%= f.label :changedFrom %>
    <%= f.text_field :changedFrom %>
  </div>

  <div class="field">
    <%= f.label :syncedFrom %>
    <%= f.number_field :syncedFrom %>
  </div>

  <div class="field">
    <%= f.label :deleteFlag %>
    <%= f.check_box :deleteFlag %>
  </div>

  <div class="field">
    <%= f.label :user_id %>
    <%= f.text_field :user_id %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

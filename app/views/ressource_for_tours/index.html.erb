<p id="notice"><%= notice %></p>

<h1>Ressource For Tours</h1>

<table>
  <thead>
    <tr>
      <th>Createdfrom</th>
      <th>Picture</th>
      <th>Changedfrom</th>
      <th>Syncedfrom</th>
      <th>Deleteflag</th>
      <th>Tour</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ressource_for_tours.each do |ressource_for_tour| %>
      <tr>
        <td><%= ressource_for_tour.createdFrom %></td>
        <td><%= ressource_for_tour.picture %></td>
        <td><%= ressource_for_tour.changedFrom %></td>
        <td><%= ressource_for_tour.syncedFrom %></td>
        <td><%= ressource_for_tour.deleteFlag %></td>
        <td><%= ressource_for_tour.Tour %></td>
        <td><%= link_to 'Show', ressource_for_tour %></td>
        <td><%= link_to 'Edit', edit_ressource_for_tour_path(ressource_for_tour) %></td>
        <td><%= link_to 'Destroy', ressource_for_tour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ressource For Tour', new_ressource_for_tour_path %>

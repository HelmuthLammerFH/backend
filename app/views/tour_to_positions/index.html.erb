<p id="notice"><%= notice %></p>

<h1>Tour To Positions</h1>

<table>
  <thead>
    <tr>
      <th>Startdate</th>
      <th>Enddate</th>
      <th>Createdfrom</th>
      <th>Changedfrom</th>
      <th>Syncedfrom</th>
      <th>Deleteflag</th>
      <th>Tour</th>
      <th>Tourposition</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tour_to_positions.each do |tour_to_position| %>
      <tr>
        <td><%= tour_to_position.startDate %></td>
        <td><%= tour_to_position.endDate %></td>
        <td><%= tour_to_position.createdFrom %></td>
        <td><%= tour_to_position.changedFrom %></td>
        <td><%= tour_to_position.syncedFrom %></td>
        <td><%= tour_to_position.deleteFlag %></td>
        <td><%= tour_to_position.tour %></td>
        <td><%= tour_to_position.TourPosition %></td>
        <td><%= link_to 'Show', tour_to_position %></td>
        <td><%= link_to 'Edit', edit_tour_to_position_path(tour_to_position) %></td>
        <td><%= link_to 'Destroy', tour_to_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tour To Position', new_tour_to_position_path %>
